        </bindings>
        <client>
            <endpoint address="http://localhost:50010/JsonAPI.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IJsonAPI" contract="MooAPI.IJsonAPI"
                name="BasicHttpBinding_IJsonAPI" />
        </client>
    </system.serviceModel>
</configuration>